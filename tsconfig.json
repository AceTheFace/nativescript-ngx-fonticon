{
    "compilerOptions": {
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noEmitHelpers": true,
        "noEmitOnError": false,
        "declaration": true,
        "lib": ["es2016"]
    },
    "exclude": [
        "node_modules",
        "src",
        "bundles"
    ]
}
